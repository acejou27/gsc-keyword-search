{
  "name": "gsc-keyword-search-v2",
  "version": "2.0.0",
  "description": "GSC關鍵字搜索工具 - 雲端版本，支援 GitHub Actions 和 Cloudflare Workers",
  "main": "index.js",
  "scripts": {
    "start": "node .github/scripts/search.js",
    "test": "jest",
    "dev:worker": "cd cloudflare-worker && wrangler dev",
    "deploy:worker": "cd cloudflare-worker && wrangler deploy",
    "generate-report": "node .github/scripts/generate-report.js",
    "setup": "npm install && cd cloudflare-worker && npm install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/gsc-keyword-search.git"
  },
  "keywords": [
    "seo",
    "keyword-search",
    "google-search",
    "web-scraping",
    "github-actions",
    "cloudflare-workers",
    "automation",
    "puppeteer"
  ],
  "author": "GSC Team",
  "license": "MIT",
  "dependencies": {
    "puppeteer": "^21.0.0",
    "csv-parser": "^3.0.0"
  },
  "devDependencies": {
    "jest": "^29.0.0",
    "@types/node": "^20.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "bugs": {
    "url": "https://github.com/your-username/gsc-keyword-search/issues"
  },
  "homepage": "https://github.com/your-username/gsc-keyword-search#readme"
}